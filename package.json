{
    "name": "vscode-sql-template-literal-ts",
    "displayName": "vscode-sql-template-literal-ts",
    "description": "Syntax highlighting for sql in template literals",
    "version": "0.1.0",
    "publisher": "abstractalgo",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/abstractalgo/vscode-sql-template-literal-ts.git"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "grammars": [
            {
                "injectTo": [
                    "source.js",
                    "source.js.jsx",
                    "source.jsx",
                    "source.ts",
                    "source.tsx"
                ],
                "scopeName": "inline.lit-sql",
                "path": "./syntaxes/lit-sql.json",
                "embeddedLanguages": {
                    "meta.embedded.block.sql": "sql"
                }
            }
        ]
    }
}